{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index" }
  ],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 60,
      "excludeFiles": "{**/__pycache__/**,**/*.pyc,**/*.pyo,data/**,tests/**,__tests__/**,docs/**,*.zip}"
    }
  }
}
